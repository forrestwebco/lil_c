var load_more_items_link = $("#load_more_items_link")
var anchor = $("#load_more_items_anchor");

anchor.after("<%=j render "more", shared_items: @shared_items %>");
anchor.remove();

load_more_items_link.toggle();
$("#load_bar").toggle();

<% if @shared_items.empty? or @shared_items.size < page_size %>
  load_more_items_link.remove();
<% end %>

window.navigator.vibrate(1);
